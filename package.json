{
  "name": "@jarboer/electron-prompt",
  "version": "2.0.0",
  "description": "Electron helper to prompt for a value via input or select",
  "packageManager": "pnpm@9.0.0",
  "keywords": [
    "electron",
    "prompt",
    "string"
  ],
  "author": {
    "name": "Jarrett",
    "email": "serve-harrows-0k@icloud.com",
    "url": "https://github.com/Jarboer"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jarboer/electron-prompt.git"
  },
  "license": "MIT",
  "main": "lib/electron-prompt.js",
  "scripts": {
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "pnpm lint",
    "tailwind": "pnpx tailwindcss -i ./src/page/prompt-in.css -o ./src/page/prompt-out.css",
    "tailwind:watch": "pnpx tailwindcss -i ./src/page/prompt-in.css -o ./src/page/prompt-out.css --watch",
    "webpack": "pnpx webpack-cli --config webpack.config.js"
  },
  "xo": {
    "esnext": true,
    "env": [
      "node",
      "browser"
    ],
    "rules": {
      "unicorn/prefer-ternary": 0,
      "unicorn/prefer-module": 0
    }
  },
  "devDependencies": {
    "@electron/remote": "^2.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "electron": "^31.4.0",
    "html-webpack-plugin": "^5.6.0",
    "tailwindcss": "^3.4.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "xo": "^0.44.0"
  }
}
