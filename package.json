{
  "name": "@jarboer/electron-prompt",
  "version": "1.10.0",
  "description": "Electron helper to prompt for a value via input or select",
  "keywords": [
    "electron",
    "prompt",
    "string"
  ],
  "author": {
    "name": "Jarrett",
    "email": "serve-harrows-0k@icloud.com",
    "url": "https://github.com/Jarboer"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jarboer/electron-prompt.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "pnpm lint",
    "tailwind": "pnpx tailwindcss -i ./lib/page/prompt-in.css -o ./lib/page/prompt-out.css",
    "tailwind:watch": "pnpx tailwindcss -i ./lib/page/prompt-in.css -o ./lib/page/prompt-out.css --watch"
  },
  "xo": {
    "esnext": true,
    "env": [
      "node",
      "browser"
    ],
    "rules": {
      "unicorn/prefer-ternary": 0,
      "unicorn/prefer-module": 0
    }
  },
  "devDependencies": {
    "tailwindcss": "^3.4.10",
    "xo": "^0.44.0"
  }
}
